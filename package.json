{
  "name": "qr-inventory",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rm -rf",
    "clean:all": "yarn run clean:vendors && yarn run clean:dist",
    "clean:vendors": "yarn run clean node_modules",
    "clean:dist": "yarn run clean dist",
    "clean:install": "yarn run clean:all && yarn",
    "start": "yarn run start:sandbox",
    "start:base": "yarn run update:serve && ng serve",
    "start:sandbox": "yarn run start:base --configuration sandbox",
    "start:prod": "yarn run start:base --configuration production",
    "build": "yarn run build:sandbox",
    "build:base": "ng build --extra-webpack-config webpack.partial.js -o",
    "build:sandbox": "yarn run build:base --configuration sandbox",
    "build:prod": "yarn run build:base --configuration production",
    "script:app": "node scripts/app.js --file src/app.json",
    "update:serve": "yarn run script:app --type serve",
    "update:patch": "yarn run script:app --type patch",
    "update:minor": "yarn run script:app --type minor",
    "update:major": "yarn run script:app --type major",
    "bundle:analyzer": "webpack-bundle-analyzer dist/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~11.0.5",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/router": "~11.0.5",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@hackages/ngxerrors": "^8.0.0",
    "angular2-ladda": "^3.1.1",
    "animate.css": "^4.1.1",
    "bootstrap": "^4.5.3",
    "html5-qrcode": "^1.2.4",
    "jquery": "^3.5.1",
    "ladda": "^2.0.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "ngx-bootstrap": "^6.2.0",
    "ngx-build-plus": "^11.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@types/jasmine": "~3.6.0",
    "@types/lodash": "^4.14.167",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "commander": "^6.2.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
